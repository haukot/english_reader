{
  "name": "translation_app",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && parcel build index.html && yarn run postbuild",
    "postbuild": "mkdir -p dist/ && cp -r static dist/",
    "start": "yarn run postbuild && yarn run parcel serve -p 8888 index.html"
  },
  "parcelIgnore": [
    "/static/words_dict/.+"
  ],
  "devDependencies": {
    "parcel": "^2.8.2",
    "parcel-resolver-ignore": "^2.1.3",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "> 1.5%"
  ],
  "dependencies": {
    "dexie": "^3.0.2",
    "google-translate-api-browser": "^1.1.71",
    "wink-lemmatizer": "^3.0.1",
    "yarn": "^1.22.19"
  }
}
